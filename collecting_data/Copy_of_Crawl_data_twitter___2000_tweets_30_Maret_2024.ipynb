      "cell_type": "code",
      "source": [
        "# Crawl Data\n",
        "\n",
        "filename = 'cina2020.csv'\n",
        "search_keyword = 'cina kontol since:2020-01-01 until:2022-06-31 lang:id'\n",
        "limit = 5000\n",
        "!npx -y tweet-harvest@2.6.1 -o \"{filename}\" -s \"{search_keyword}\" --tab \"LATEST\" -l {limit} --token {twitter_auth_token}"
      ],
